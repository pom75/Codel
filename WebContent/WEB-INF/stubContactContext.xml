	<bean id="PhoneNumberExp1" class="domain.PhoneNumber" scope="prototype">
		<property name="phoneKind" value="home" />
		<property name="phoneNumber" value="0189625412" />
	</bean>

	<bean id="PhoneNumberExp2" class="domain.PhoneNumber" scope="prototype">
		<property name="phoneKind" value="office" />
		<property name="phoneNumber" value="0165971256" />
	</bean>

	<bean id="PhoneNumberExp3" class="domain.PhoneNumber" scope="prototype">
