<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NewContact</servlet-name>
		<servlet-class>servlets.NewContactServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteContact</servlet-name>
		<servlet-class>servlets.DeleteContactServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UpdateContact</servlet-name>
		<servlet-class>servlets.UpdateContactServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SearchContact</servlet-name>
		<servlet-class>servlets.SearchContactServlet</servlet-class>
	</servlet>
	

	<servlet-mapping>
		<servlet-name>NewContact</servlet-name>
		<url-pattern>/newcontact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteContact</servlet-name>
		<url-pattern>/deletecontact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UpdateContact</servlet-name>
		<url-pattern>/updatecontact</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SearchContact</servlet-name>
		<url-pattern>/searchcontact</url-pattern>
	</servlet-mapping>
</web-app>